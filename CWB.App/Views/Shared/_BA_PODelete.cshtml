                <input type="text" hidden id="PODPartId" name="PartId" value="0" />
            <div class="modal-footer">
                <button id="BtnPODelete" type="button" class="btn btn-primary btn-sm">Save</button>
            </div>
                
            </form>
        </div>
    </div>
</div>
