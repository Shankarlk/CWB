<div class="modal fade" id="existing-part-bof" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog bxs-modal">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="standard-modalLabel">Select Part</h4>
                <button type="button" class="btn-close" id="btn-close-ExistingPartsBOF" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <!-- ---------------- -->
                <div class="row mt-3">
                    <label class="form-label col-md-4 mb-2"> Company  </label>
                    <div class="col-md-8 mb-2">
                        <input class="form-control form-control-sm" type="text" placeholder="Enter here" value="" id="bofco" name="bofco" />
                    </div>
                    <label class="form-label col-md-4 mb-2">Part Number </label>
                    <div class="col-md-8 mb-2">
                        <input class="form-control form-control-sm" type="text" placeholder="Enter here" value="" id="bofpartno" name="bofpartno" />
                    </div>
                    <!-- ========================= -->
                    <label class="form-label col-md-4 mb-2">Part Description </label>
                    <div class="col-md-8 mb-2">
                        <input class="form-control form-control-sm" type="text" placeholder="Enter here" value="" id="bofpartdescription" name="bofpartdescription" />
                    </div>
                    <!-- ========================= -->
                   
                    <div class="col-md-4 mt-1">
                        <label class="form-label mb-1 font-13">Child Part</label>
                        <input class="form-check-input m-l-5" value="child" type="radio" name="ExistingPartBOFRadio" title="" data-plugin="tippy" data-tippy-placement="top" checked id="child">
                    </div>
                    <div class="col-md-4 mt-1">
                        <label class="form-label mb-1 font-13">Assembly</label>
                        <input class="form-check-input m-l-5" value="assembly" type="radio" name="ExistingPartBOFRadio" title="" data-plugin="tippy" data-tippy-placement="top" id="assy">
                    </div>
                    <div class="col-md-4 mt-1">
                        <label class="form-label mb-1 font-13">BOF</label>
                        <input class="form-check-input m-l-5" value="bof" type="radio" name="ExistingPartBOFRadio" title="" data-plugin="tippy" data-tippy-placement="top" id="bof">
                    </div>
                    <!-- ========================= -->
                    <!--<div class="col-md-4"></div>-->
                    <!-- ========================= -->
                    <!-- ========================= -->
                    <div class="col-md-6 mt-1 text-end">
                    </div>
                    <!-- ========================= -->
                </div>
                <!-- ============================== -->
                <div id="DIV_CHLDTABLE" class="table-responsive table-he-250 mt-3">
                    <table class="table table-sm table-bordered  mb-3 mt-1 tableFixHead" id="tbl-existingparts">
                        <thead class="table-info th-sti text-center">
                            <tr class="table-border-bottom">
                                <th width="10%"></th>
                                <th width="30%">Part No.</th>
                                <th width="40%">Description</th>
                                <th width="15%">Child/Assy</th>
                                <th width="5%"></th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
                <div id="DIV_ASSMTABLE" class="table-responsive table-he-250 mt-3">
                    <table class="table table-sm table-bordered  mb-3 mt-1 tableFixHead" id="tbl-existingparts_assembly">
                        <thead class="table-info th-sti text-center">
                            <tr class="table-border-bottom">
                                <th width="10%"></th>
                                <th width="30%">Part No.</th>
                                <th width="40%">Description</th>
                                <th width="15%">Child/Assy</th>
                                <th width="5%"></th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
                <div id="DIV_BOFTABLE" class="table-responsive table-he-250 mt-3">
                    <table class="table table-sm table-bordered  mb-3 mt-1 tableFixHead" id="tbl-existingpartsbof">
                        <thead class="table-info th-sti text-center">
                            <tr class="table-border-bottom">
                                <th width="10%"></th>
                                <th width="25%">Part Number</th>
                                <th width="30%">Description</th>
                                <th width="30%">BOF Type</th>
                                <th width="5%"></th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
                <!-- =============================== -->
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary btn-sm" onclick="copyBOFData()">Select</button>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>

<template id="BOFPartTemplate">
    <tr>
        <td>
            <input class="form-check-input mt-1" type="radio" name="radiopartsbof" value="{num}">
        </td>
        <td class="one" data-key="partNo">{partNo}</td>
        <td class="two"  data-key="description">{description}</td>
        <td class="six" data-key="boughtOutFinishMadeType">{boughtOutFinishMadeType}</td>
   </tr>
</template>

<template id="ChildPartTemplate">
    <tr>
        <td>
            <input class="form-check-input mt-1" type="radio" name="radiopartsbof" value="{num}">
        </td>
        <td data-key="partNo">{partNo}</td>
        <td data-key="description">{description}</td>
        <td data-key="masterPartType">{masterPartType}</td>
   </tr>
</template>
<template id="AssemblyPartTemplate">
    <tr>
        <td>
            <input class="form-check-input mt-1" type="radio" name="radiopartsbof" value="{num}">
        </td>
        <td data-key="partNo">{partNo}</td>
        <td data-key="description">{description}</td>
        <td data-key="masterPartType">{masterPartType}</td>
    </tr>
</template>
