<template id="SubConNameRow">
    <tr>
        <td>
            <input type="radio" id="stepsubcon_{companyId}" name="RadioSubConName" value="{companyId}">
        </td>
        <td>{companyName}</td>
    </tr>
</template>

<template id="SubConNameRow_Checked">
    <tr>
        <td>
            <input type="radio" id="stepsubcon_{companyId}" name="RadioSubConName" value="{companyId}" checked>
        </td>
        <td>{companyName}</td>
    </tr>
</template>

<template id="TitleRow">
    <tr>
        <td>
            <span class="h6">Part No :</span> {partNo}
        </td>
        <td>
            <span class="h6">Description :</span>  {partDescription}
        </td>
        <td>
            <span class="h6">Company :</span> {companyName}
        </td>
        <td>
            <span class="h6">Routing :</span> {routingName}
        </td>
        <td>
            <span class="h6">Routing Step :</span> {stepNumber}
        </td>
    </tr>
</template>
