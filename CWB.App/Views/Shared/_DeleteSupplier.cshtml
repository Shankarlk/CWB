<div class="modal fade" id="delete-supplier"
     role="dialog" data-bs-backdrop="static"
     data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog modal-sm">
        <div class="modal-content bxs-modal">
            <div class="modal-header">
                <h4 class="modal-title" id="mySmallModalLabel">Delete Supplier</h4>
                <button id="BtnDeleteSupplierClose" type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form method="post" id="FormDelRoutingName" onsubmit="return false;">
                    <div class="row">
                        <label class="form-label col-md-5 mb-2">Supplier <span class="text-danger" id="SupplierToDelete"></span></label>
                        <div class="col-md-7 mb-1">
                            <input hidden class="form-control form-control-sm" id="DelRoutingName" name="RoutingName" type="text" title="Enter the Routing Name ... It has to be Unique" data-plugin="tippy" data-tippy-placement="top">
                            <input type="text" hidden id="DelManufacturedPartId" name="ManufacturedPartId" value="0" />
                            <input type="text" hidden id="DelOrigRoutingId" name="OrigRoutingId" value="0" />
                            <input type="text" hidden id="DelStatus" name="Status" value="Active" />
                            <input type="text" hidden id="DelPreferredRouting" name="PreferredRouting" value="0" />
                            <input type="text" hidden id="DelRoutingId" name="RoutingId" value="0" />
                        </div>
                        <div>
                            <label class="form-label col-md-8 mb-2"> Are you sure you want to delete this supplier from the step?</label>
                        </div>
                        <!-- =========  -->
                        <div class="col text-end mt-2">
                            <button id="BtnDelStepSupplier" type="button" class="btn btn-sm btn-primary">Delete</button>
                            <button type="button" id="CancelDelStepSupplier" class="btn btn-sm btn-primary" data-plugin="tippy" data-tippy-placement="top">Cancel</button>
                        </div>
                        <!-- =========  -->
                    </div>
                </form>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
